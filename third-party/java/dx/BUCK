    name = "dx",
    srcs = glob(["src/**/*.java"]),
    licenses = [
        "NOTICE",
    ],
    visibility = ["PUBLIC"],
    deps = ["//third-party/java/guava:guava"],
)

file_alias(
    name = "etc_dx",
    source = "etc/dx",
    visibility = [
        "PUBLIC",
    ],
)
